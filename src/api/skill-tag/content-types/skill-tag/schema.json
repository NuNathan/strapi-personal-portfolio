{
  "kind": "collectionType",
  "collectionName": "skill_tags",
  "info": {
    "singularName": "skill-tag",
    "pluralName": "skill-tags",
    "displayName": "SkillTag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "skill": {
      "type": "string",
      "required": true
    },
    "mainColour": {
      "type": "customField",
      "customField": "plugin::color-picker.color",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "required": true
    },
    "skillLevel": {
      "type": "decimal",
      "required": true,
      "default": 40,
      "min": 40,
      "max": 160
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "mappedBy": "skillTags"
    }
  }
}
