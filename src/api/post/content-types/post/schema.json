{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "img": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "posts"
    },
    "skillTags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::skill-tag.skill-tag",
      "inversedBy": "posts"
    },
    "type": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "project",
        "blog"
      ]
    },
    "views": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0
    },
    "readTime": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": true
    },
    "demo": {
      "type": "string"
    },
    "github": {
      "type": "string"
    }
  }
}
